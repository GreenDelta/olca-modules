<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:simpleType name="TIndexNumber">
		<xsd:annotation>
			<xsd:documentation>Unique identifier. Format: Integer (32-bit) greater then zero</xsd:documentation>
			<xsd:documentation>Eindeutige Nummer zur Identifizierung eines Prozesses. Format: Integer (32-bit) groesser 0</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TPercent">
		<xsd:annotation>
			<xsd:documentation>Format: ddd.d</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:float">
			<xsd:pattern value="\d{1,3}\.\d"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TUnit">
		<xsd:annotation>
			<xsd:documentation>20 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TFloatNumber">
		<xsd:annotation>
			<xsd:documentation>Floating Point Zahl, 64-bit Genauigkeit, mit Vorzeichen, zur Angabe von Mengen und Unsicherheiten
</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:double"/>
	</xsd:simpleType>
	<xsd:simpleType name="TString20">
		<xsd:annotation>
			<xsd:documentation>20 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString30">
		<xsd:annotation>
			<xsd:documentation>30 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="30"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString40">
		<xsd:annotation>
			<xsd:documentation>40 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="40"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString80">
		<xsd:annotation>
			<xsd:documentation>80 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="80"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString255">
		<xsd:annotation>
			<xsd:documentation>255 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString32000">
		<xsd:annotation>
			<xsd:documentation>Free text for additional remarks, detailed descriptions etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="32000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TCompanyCode">
		<xsd:annotation>
			<xsd:documentation>7 letter company code (capital letters).</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="7"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TRegionalCode">
		<xsd:annotation>
			<xsd:documentation>7 letter regional code (capital letters). Enlarged list of 2 letter ISO country codes by codes for regions, continents, market areas and organisations and companies. </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="7"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="TPerson">
		<xsd:annotation>
			<xsd:documentation>Used for the identification of members of the organisation / institute co-operating within a quality network (e.g., ecoinvent) referred to in the areas Validation, dataEntryBy and dataGeneratorAndPublication.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="number" type="TIndexNumber" use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5800</spoldID>
					<type>Number (Integer)</type>
					<size>10</size>
					<options/>
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>Yes</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>ID number is attributed to each person of an organisation/institute co-operating in a quality network such as ecoinvent. It is used to identify persons cited within one dataset.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="name" type="TString40" use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5802</spoldID>
					<type>Text</type>
					<size>40</size>
					<options/>
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>Yes</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>Name and surname of the person working in an organisation/institute which is a member of the quality network.</xsd:documentation>
				<xsd:documentation>Identifies the person together with 'address' (#5803).</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="address" type="TString255" use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5803</spoldID>
					<type>Text</type>
					<size>255</size>
					<options/>
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>Yes</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>Complete address, including street, po-box (if applicable), zip-code, city, state (if applicable), country.</xsd:documentation>
				<xsd:documentation>Identifies the person together with 'name' (#5802).</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="telephone" type="TString40" use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5804</spoldID>
					<type>Text</type>
					<size>40</size>
					<options/>
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>Yes</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>Phone number including country and regional codes.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="telefax" type="TString40" use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5805</spoldID>
					<type>Text</type>
					<size>40</size>
					<options/>
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>No</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>Fax number including country and regional codes.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="email" type="TString80" use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5806</spoldID>
					<type>Text</type>
					<size>80</size>
					<options/>
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>No</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>Complete email address.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="companyCode" type="TCompanyCode" use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5807</spoldID>
					<type>Text</type>
					<size>7</size>
					<options/>
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>Yes</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>7 letter company code of the organisation/institute co-operating in a quality network.</xsd:documentation>
				<xsd:documentation>Identifies the co-operation partner together with the countryCode (#5808).</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="countryCode" type="TISOCountryCode" use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5808</spoldID>
					<type>Text</type>
					<size>2</size>
					<options>ISO-Codes</options>
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>Yes</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>2 letter ISO-country code of the organisation/institute co-operating in a quality network.</xsd:documentation>
				<xsd:documentation>Identifying the co-operation partner together with the companyCode (#5807).</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
