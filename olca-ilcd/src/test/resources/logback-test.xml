<configuration debug="false">
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} %green([%thread]) %highlight(%level) %logger{50} - %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="org.openlca" level="ALL"/>

	<root level="WARN">
		<appender-ref ref="STDOUT"/>
	</root>
</configuration>
