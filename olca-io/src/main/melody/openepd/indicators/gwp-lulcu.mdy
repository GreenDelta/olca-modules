let .climate_change = {
  ahead {
    any of <char>;
    "climate";
  }
  ahead {
    any of <char>;
    "change";
  }
  any of <char>;
}


let .global_warming = {
  ahead {
    any of <char>;
    "global";
  }
  ahead {
    any of <char>;
    "warming";
  }
  any of <char>;
}

let .gwp = {
  any of <char>;
  any of not <word>;
  "gwp";
  any of not <word>;
  any of <char>;
}

either {
  .climate_change;
  .global_warming;
  .gwp;
}
"land";
